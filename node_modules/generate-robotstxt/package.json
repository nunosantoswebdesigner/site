    "dist"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 8.9.0"
  },
  "dependencies": {
    "cosmiconfig": "^5.2.1",
    "fs-extra": "^8.1.0",
    "ip-regex": "^4.1.0",
    "is-absolute-url": "^3.0.0",
    "meow": "^5.0.0",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.5.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "eslint": "^6.0.1",
    "eslint-plugin-ava": "^7.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-itgalaxy": "^103.0.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-unicorn": "^9.1.1",
    "execa": "^2.0.2",
    "husky": "^3.0.0",
    "jest": "^24.5.0",
    "lint-staged": "^9.0.2",
    "npm-run-all": "^4.0.0",
    "prettier": "^1.18.2",
    "remark-cli": "^6.0.1",
    "remark-preset-lint-itgalaxy": "^14.0.0",
    "rimraf": "^2.5.2",
    "standard-version": "^6.0.1",
    "tempfile": "^3.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --ignore /__tests__/",
    "lint:prettier": "prettier --list-different '{src,__tests__}/**/*.{js,mjs,jsx,md,yml,yaml}' '*.{js,mjs,md,yml,yaml}' '!CHANGELOG.md'",
    "lint:js": "eslint . --cache --report-unused-disable-directives --ignore-path .gitignore --ext '.js,.mjs,.jsx,md'",
    "lint:md": "remark . -i .gitignore -f -q",
    "lint": "npm-run-all -l -p 'lint:**'",
    "prettier": "npm run lint:prettier -- --write",
    "fix:eslint": "npm run lint:js -- --fix",
    "fix": "npm-run-all -l prettier -p 'fix:**'",
    "prepare": "npm run build",
    "release": "standard-version",
    "test:only": "jest",
    "test:coverage": "jest --coverage",
    "pretest": "npm run lint",
    "test": "npm run test:only"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "helpers.js"
    ]
  }
}
